<h1>Validation</h1>

<form>
    <h2>Phone Number</h2>
    <input min="9" max="9" type="number" required/>

    <h2>FirstName</h2>
    <input name="FirstName" type="text" required />
    <h2>LastName</h2>
    <input name="Last Name" type="text" required />

    <h2>Password</h2>
    <input name="Pasword" type="password" maxlength="7" required />
    <h2>Email</h2>
    <input name="Email" type="email" maxlength="20" required />
    <h2>Age</h2>
    <input name="Age" type="number" min=0 max=120 required />
    <input type="submit" />
</form>